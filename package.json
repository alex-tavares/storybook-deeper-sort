{
  "name": "storybook-deeper-sort",
  "version": "2.0.2",
  "description": "Adds deeper Storybook story sorting and story order control with wildcards.",
  "repository": "git@github.com:alex-tavares/storybook-deeper-sort.git",
  "author": "Alex Tavares",
  "type": "module",
  "license": "MIT",
  "main": "lib/index.js",
  "exports": {
    ".": "./lib/index.js"
  },
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "postversion": "git push --follow-tags",
    "prepublishOnly": "yarn build",
    "build": "babel ./src --out-dir ./lib --ignore **/*.test.js,**/stories",
    "format": "prettier --write ./src",
    "lint": "eslint  --fix ./src",
    "test": "jest ./src",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "storybook": "^10.0.0"
  },
  "browserslist": [
    "defaults"
  ],
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.8",
    "@babel/preset-react": "^7.24.7",
    "@storybook/addon-docs": "^10.0.8",
    "@storybook/react-vite": "^10.0.8",
    "babel-jest": "^29.7.0",
    "codecov": "^3.8.3",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-storybook": "^0.6.12",
    "jest": "^29.5.0",
    "prettier": "2.7.1",
    "prop-types": "^15.8.1",
    "storybook": "^10.0.8",
    "vite": "^5.4.21"
  },
  "keywords": [
    "storybook-addons",
    "storybook-addon",
    "storybook",
    "addon",
    "sort",
    "sorting",
    "story-sort",
    "story-sorting",
    "story-order",
    "story-ordering",
    "order",
    "deep-sort",
    "deeper-sort"
  ],
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
