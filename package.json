{
  "name": "storybook-deeper-sort",
  "version": "1.1.0",
  "description": "Powers up stories sort by providing more levels of control.",
  "repository": "git@github.com:alex-tavares/storybook-deeper-sort.git",
  "author": "Alex Tavares",
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "postversion": "git push --follow-tags",
    "prepublishOnly": "yarn build",
    "build": "babel ./src --out-dir ./lib --ignore **/*.test.js,**/stories",
    "format": "prettier --write ./src",
    "lint": "eslint  --fix ./src",
    "test": "jest ./src",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "browserslist": [
    "defaults"
  ],
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@storybook/addon-essentials": "^7.0.22",
    "@storybook/addon-interactions": "^7.0.22",
    "@storybook/addon-links": "^7.0.22",
    "@storybook/blocks": "^7.0.22",
    "@storybook/react": "^7.0.22",
    "@storybook/react-vite": "^7.0.22",
    "codecov": "^3.8.3",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-storybook": "^0.6.12",
    "jest": "^29.5.0",
    "prettier": "2.7.1",
    "prop-types": "^15.8.1",
    "storybook": "^7.0.22",
    "vite": "^4.3.9"
  },
  "keywords": [
    "storybook-addons",
    "storybook-addon",
    "storybook",
    "addon",
    "sort",
    "deep-sort",
    "deeper-sort"
  ],
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
