    "codecov": "^3.8.3",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^28.1.3",
    "prettier": "2.7.1"
  },
  "keywords": [
    "storybook-addons",
    "storybook-addon",
    "storybook",
    "addon",
    "sort",
    "deep-sort",
    "deeper-sort"
